{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "composite": true,
    "target": "ES2022",
    "useDefineForClassFields": true,
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "NodeNext",
    "types": ["vite/client"],
    "skipLibCheck": true,
    "moduleResolution": "NodeNext",
    "baseUrl": ".",
    "paths": {
      "@fluentui/react-components": ["./node_modules/@fluentui/react-components"],
      "@fluentui/react-icons": ["./node_modules/@fluentui/react-icons"],
      "@azure/msal-browser": ["./node_modules/@azure/msal-browser"],
      "@azure/msal-react": ["./node_modules/@azure/msal-react"]
    },
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "moduleDetection": "force",
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "erasableSyntaxOnly": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true
  },
  "include": ["src"]
}
